, {\field{\*\fldinst{HYPERLINK \\l Par635  \\o "Ссылка на текущий документ"}}{\fldrslt\cf2 22.2}}
 и {\field{\*\fldinst{HYPERLINK \\l Par654  \\o "Ссылка на текущий документ"}}{\fldrslt\cf2 22.3}}
